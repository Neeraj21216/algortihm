// Online C++ compiler to run C++ program online
#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void merge(vector<int>&arr,int l,int mid,int h){
    //
    vector<int>ans;
    int i=l;
    int j=mid+1;
    while(i<=mid && j<=h){
        //
        if(arr[i]<=arr[j]){
            ans.push_back(arr[i]);
            i++;
        }
        else if(arr[i]>arr[j]){
            ans.push_back(arr[j]);
            j++;
        }
    }
    //i<
    while(i<=mid){
        ans.push_back(arr[i]);
        i++;
    }
    while(j<=h){
        ans.push_back(arr[j]);
        j++;
    }
    for(int i=0;i<ans.size();i++){
        int index=i+l;
        arr[index]=ans[i];
        

    }
    return ;
    
}
void mergesort(vector<int>& arr,int l ,int h){
    if(l>=h){
        //means it is sorte d
        return ;
    }
    int  mid=(l+h)/2;
    mergesort(arr,l,mid);
    mergesort(arr,mid+1,h);
    merge(arr,l,mid,h);
}
int main() {
    // Write C++ code here4
    vector<int>arr={3,1,2,4,1,5,6,2,5};
    int l=0;
    int h=arr.size()-1;
    mergesort(arr,l,h);
    for(auto i:arr){
        cout<<i<<endl;
    }

    return 0;
}
